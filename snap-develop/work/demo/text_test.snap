import sound.sampled.AudioFormat;
import sound.sampled.AudioInputStream;
import sound.sampled.AudioSystem;
import util.concurrent.atomic.AtomicInteger;

class OuterClass{

   var fireball: Integer = 0;
   
   public test(){
      var func = () ->{
         println(this);
      };
      func();
      fireball++;
      return this;
   }
   public dump(){
      println(fireball);
   }
}
   
new OuterClass().test().dump();

