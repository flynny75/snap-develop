class Table {

   const list;
   new(){
      this.list =[];
   }
   create(key, cells){
      var r: Row = new StringRow(key,cells);
      list.add(r);
   }
   dump(){
      for(var row in list){
         row.dump();
      }
   }

   trait Row{
      dump();
   }
   
   class StringRow with Row {
      const key;
      const cells;
      new(key, cells){
         this.key = key;
         this.cells = cells;
      }
      dump(){
         for(var cell in cells){
            print("[${cell}]");
         }
         println();
      }
   }
}
println(Table.Row.class);
println(Table.StringRow.class);

var table = new Table();
table.create("a", [1,2,3,4,5]);
table.create("b", ['a','b','c','d']);
table.create("c", ['X','Y','Z']);
table.dump();