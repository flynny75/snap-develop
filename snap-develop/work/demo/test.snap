class Foo{
   toString(){
      "x=${x}";
   }
}
class Bar extends Foo{
   const x;
   const y;
   new(x){
      this.x=x;
      this.y=x+2;
   }
   toString(){
      "x=${x}";
   }
}
var f =new Foo();
var start=System.currentTimeMillis();
for(var i in 0..1000000){
   new Bar(i);
}
var finish=System.currentTimeMillis();
var duration=finish-start;
var secs = duration/1000d;
var times = 1000000d/secs;
println("${times} per sec");